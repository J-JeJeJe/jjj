<h4>配送先編集</h4>

<%= form_with model: @shipping_address, url: shipping_address_path, local:true do |f| %>
 <label for="postal_code">郵便番号（ハイフンなし）</label>
 <%= f.text_field :postal_code %><br/>
 <label for="address">住所</label>
 <%= f.text_field :address %><br/>
 <label for="name">宛名</label>
 <%= f.text_field :name %>
 
 <%= f.submit '変更内容を保存' %>
<% end %>
